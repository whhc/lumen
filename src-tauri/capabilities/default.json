{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main",
    "media-*"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "core:webview:allow-create-webview-window",
    "core:webview:allow-webview-close",
    "core:window:default",
    "core:window:allow-close",
    "dialog:default",
    "dialog:allow-open",
    "log:default"
  ]
}